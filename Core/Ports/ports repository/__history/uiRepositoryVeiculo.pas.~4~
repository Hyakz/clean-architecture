unit uiRepositoryVeiculo;

interface

uses
  uVeiculo, uDTOVeiculo, System.Generics.Collections;

type
  IRepositoryVeiculo = interface
    procedure Cadastrar(Cliente: TCliente);
    procedure Alterar(Cliente: TCliente);
    procedure Excluir(Codigo: Integer);
    function Consultar(Dto: DtoCliente): TList<TCliente>;
  end;

implementation

end.
